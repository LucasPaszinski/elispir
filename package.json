{
  "name": "elispir",
  "displayName": "elispir",
  "description": "Eval code to terminal from editor",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:elispir.sendText",
    "onCommand:elispir.sendFile",
    "onCommand:elispir.reload",
    "onCommand:elispir.sendAllLinesAbove"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "elispir.reload",
        "title": "reload"
      },
      {
        "command": "elispir.sendText",
        "title": "sendText"
      },
      {
        "command": "elispir.sendFile",
        "title": "sendFile"
      },
      {
        "command": "elispir.sendAllLinesAbove",
        "title": "sendAllLinesAbove"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Python REPL",
      "properties": {
        "elispir.iexCommand": {
          "type": "string",
          "default": "iex -S mix",
          "description": "The command that is ran when starting iex"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4",
    "@vscode/test-electron": "^2.2.2"
  }
}